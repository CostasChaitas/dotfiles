Include /Users/konstantinoschaitas/.colima/ssh_config

Host *
        IgnoreUnknown UseKeychain
        ForwardAgent yes
        AddKeysToAgent yes
        UseKeychain yes
        StrictHostKeyChecking no
        UserKnownHostsFile /dev/null
        ServerAliveInterval 120
        HostkeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa

Host github.com-verve
    HostName github.com
    User git
    IdentityFile ~/.ssh/github-verve

Host github.com-xaitakon
   HostName github.com
   User git
   IdentityFile ~/.ssh/github-xaitakon
